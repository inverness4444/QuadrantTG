{
  "name": "quadrant-telegram-bot",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node build/server.js",
    "build": "tsc --project tsconfig.json",
    "worker:dev": "tsx watch src/worker.ts",
    "worker": "node build/worker.js",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@grammyjs/ratelimiter": "1.2.1",
    "bullmq": "3.16.2",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "express-rate-limit": "7.4.0",
    "express-slow-down": "3.0.1",
    "grammy": "1.28.0",
    "ioredis": "5.4.1",
    "opossum": "9.0.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "proxy-addr": "2.0.7",
    "rate-limit-redis": "4.2.0",
    "undici": "6.19.8",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/jest": "^30.0.0",
    "@types/opossum": "8.1.9",
    "@types/proxy-addr": "2.0.2",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsx": "4.16.2",
    "typescript": "5.5.4",
    "vitest": "^4.0.8"
  }
}
